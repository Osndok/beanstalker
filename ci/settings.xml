<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0
                      http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <profiles>
        <profile>
            <id>default</id>
            <properties>
                <beanstalk.keyName>${env.EC2_KEY_NAME}</beanstalk.keyName>
                <beanstalk.iamInstanceProfile>aws-elasticbeanstalk-ec2-role</beanstalk.iamInstanceProfile>
                <beanstalk.instanceType>m1.small</beanstalk.instanceType>
                <beanstalk.s3Bucket>${env.AWSEB_S3_BUCKET}</beanstalk.s3Bucket>
                <beanstalk.silentUpload>true</beanstalk.silentUpload>
            </properties>
        </profile>
        <profile>
            <id>sonar</id>
            <properties>
                <sonar.host.url>http://${env.SONAR_HOST}</sonar.host.url>
                <sonar.jdbc.url>jdbc:mysql://${env.SONAR_HOST}/sonar?useUnicode=true&amp;characterEncoding=utf8&amp;rewriteBatchedStatements=true&amp;useConfigs=maxPerformance</sonar.jdbc.url>
                <sonar.jdbc.driver>com.mysql.jdbc.Driver</sonar.jdbc.driver>
                <sonar.jdbc.username>sonar</sonar.jdbc.username>
                <sonar.jdbc.password>${env.SONAR_JDBC_PASSWORD}</sonar.jdbc.password>
            </properties>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>default</activeProfile>
        <activeProfile>sonar</activeProfile>
    </activeProfiles>
</settings>
